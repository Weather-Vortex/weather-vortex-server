  },
  "homepage": "https://github.com/Weather-Vortex/weather-vortex-server#readme",
  "dependencies": {
    "axios": "^0.24.0",
    "bcrypt": "^5.0.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "express": "^4.17.1",
    "express-actuator": "^1.8.0",
    "express-session": "^1.17.2",
    "jsonwebtoken": "^8.5.1",
